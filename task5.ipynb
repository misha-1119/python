    "class Writer:\n",
    "    @classmethod\n",
    "    def write(cls, data, path=\"data.csv\"):\n",
    "        with open(path, 'w') as f:\n",
    "            f.write(data)\n",
    "\n",
    "    @classmethod\n",
    "    def read(cls, path=\"data.csv\"):\n",
    "        with open(path, 'r') as f:\n",
    "            return f.read()\n",
    "\n"
